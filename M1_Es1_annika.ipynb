    "import re\n",
    "\n",
    "def rime(word):\n",
    "    res=re.search('a|e|i|o|u',word)\n",
    "    position=word.find(res[0])\n",
    "    suf=word[position:len(word)]\n",
    "    prefix=['sh','f','l','m','fr','t','undert']\n",
    "    \n",
    "    for i in prefix:\n",
    "        print(i+suf)\n",
    "        \n",
    "res=rime('cake')       "
